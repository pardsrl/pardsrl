{% extends '::base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('highcharts.4.2.4/js/highcharts.js') }}"></script>
{% endblock %}

{#{%  set __title = 'Mi Dashboard' %}#}

{% block content %}

    <!-- Info boxes -->
    <div class="row">

        <div class="col-md-12">
            <div class="box box-warning">
                <!-- form start -->
                <form class="form-inline">
                    <div class="box-body">
                        <div id="form-rangos-intervenciones">
                            <div class="form-group">
                                <label for="exampleInputName2">Desde</label>
                                <input ype="text" class="datepicker" id="exampleInputName2" placeholder="">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputEmail2">Hasta</label>
                                <input type="text" class="datepicker" id="exampleInputEmail2" placeholder="">
                            </div>
                            <button type="submit" class="btn btn-primary">Filtrar</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>

    </div><!-- /.row -->

    <div class="row">

        {{ render(controller('AppBundle:Estadistica:operacionesPorEquipo')) }}

        {{ render(controller('AppBundle:Estadistica:operacionesPorYacimiento')) }}

        {{ render(controller('AppBundle:Estadistica:promediosCanosHora')) }}

        {{ render(controller('AppBundle:Estadistica:factorTiempoUtil')) }}

        {{ render(controller('AppBundle:Estadistica:promediosVarillasHora')) }}

    </div><!-- /.row -->
{% endblock %}